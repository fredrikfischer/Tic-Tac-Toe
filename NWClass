using System;
using System.Collections.Generic;
using System.Text;

namespace Super_Tic_Tac_Toe_New
{
    class NWClass
    {
        string[] NWstringArr = new string[9];
        DeclareWinnerClass DeclareWinnerClass = new DeclareWinnerClass();
        CheckWInnerOfSmallBoard CheckWInnerOfSmallBoard = new CheckWInnerOfSmallBoard();
        public void NWMethod(string splitList, string XorO)
        {
            
            switch (splitList)
            {
                case "NW":
                    NWstringArr[0] = XorO;
                    break;
                case "NC":
                    NWstringArr[1] = XorO;
                    break;
                case "NE":
                    NWstringArr[2] = XorO;
                    break;
                case "CW":
                    NWstringArr[3] = XorO;
                    break;
                case "CC":
                    NWstringArr[4] = XorO;
                    break;
                case "CE":
                    NWstringArr[5] = XorO;
                    break;
                case "SW":
                    NWstringArr[6] = XorO;
                    break;
                case "SC":
                    NWstringArr[7] = XorO;
                    break;
                case "SE":
                    NWstringArr[8] = XorO;
                    break;
            }

            CheckWInnerOfSmallBoard.MainsmallBoardCheckIfWinner("NW", NWstringArr);
        }
    }
}
